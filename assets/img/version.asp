                            }
                        }
                    }

                    $.cookie.raw = true;
                    $.cookie("enEComProdDetails", enEComProdValue, { path: '/' });
                    //End of Add to Basket click


                    //Add to Cart
                    var listName = $.trim($.cookie('EnhEcomListName'));
                    if (listName == undefined) {
                        listName = "";
                    }

                    var prodDetailsCookie = $.trim($.cookie('enEComProdDetails'));

                    if (prodDetailsCookie != undefined && prodDetailsCookie != "") {
                        var prodDetails = prodDetailsCookie.split('~');
